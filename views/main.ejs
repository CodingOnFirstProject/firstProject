        <div id="carouselExampleControls" class="carou carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="/static/image/nature1.jpg" class="mainPic" alt="자연">
                </div>
                <div class="carousel-item">
                    <img src="/static/image/nature2.jpg" class="mainPic" alt="자연">
